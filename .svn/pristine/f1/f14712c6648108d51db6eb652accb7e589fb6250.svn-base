<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    android:layout_width="match_parent"
	android:layout_height="match_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/main_view_bg"
    >
    <ImageView 
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/ic_launcher"
        android:layout_marginLeft="5dp"
        />
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:id="@+id/act_main_title_relativelayout"
	    android:background="@drawable/main_titlebar_bg"
	    >
	    <RelativeLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="80dp"
	        android:layout_marginTop="5dp"
	        android:layout_marginRight="5dp"
	        android:layout_marginBottom="35dp"
	        android:background="@drawable/main_titlebar_right_bg"
	        android:orientation="horizontal"
	        >
	        <LinearLayout 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:background="@drawable/main_title_tv_bg"
	            android:orientation="horizontal"
	            android:id="@+id/linearlayout"
	            >
	            <ImageView 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:background="@drawable/main_gem_icon2"
	                android:layout_gravity="center"
	                android:layout_marginLeft="3.5dp"
	                />
	            <TextView 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:text="6999/7000"
	                android:layout_gravity="center"
	                android:layout_marginLeft="3.5dp"
	                android:textSize="10dp"
	                android:textColor="@android:color/white"
	                android:id="@+id/act_main_experience_tv"
	                />
	        </LinearLayout>
	        <TextView 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_centerVertical="true"
	                android:layout_toRightOf="@id/linearlayout"
	                android:textSize="15dp"
	                android:textColor="@android:color/white"
	                android:id="@+id/act_main_level_tv"
	                />
	        <LinearLayout 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:layout_alignParentRight="true"
	            android:orientation="horizontal"
	            >
	            <LinearLayout 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:background="@drawable/main_title_gem_bg"
	                >
	                
	            </LinearLayout>
	            <LinearLayout 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:background="@drawable/main_title_gem_bg"
	                >
	                
	            </LinearLayout>
	            
	        </LinearLayout>
	    </RelativeLayout>
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:background="@drawable/main_task_count_bg"
	    android:layout_marginLeft="50dp"
	    android:layout_marginTop="60dp"
	    >
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="5"
	        android:textColor="@android:color/white"
	        android:layout_marginLeft="5dp"
	        android:id="@+id/act_main_task_tv"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:background="@drawable/main_titlebar_highlight"
	    />
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/act_main_title_relativelayout"
	    android:layout_alignParentRight="true"
	    android:orientation="vertical"
	    >
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="@android:color/white"
	        android:id="@+id/act_main_plan_tv"
	        />
	    <ProgressBar 
	        android:layout_width="100dp"
	        android:layout_height="wrap_content"
	        style="?android:attr/progressBarStyleHorizontal"
	        android:background="@drawable/proessbar_bg"
	        android:id="@+id/act_main_plan_pb"
	        />
	    
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="@android:color/white"
	        android:id="@+id/act_main_calorie_tv"
	        />
	    <ProgressBar 
	        android:layout_width="100dp"
	        android:layout_height="wrap_content"
	        style="?android:attr/progressBarStyleHorizontal"
	        android:background="@drawable/proessbar_bg"
	        android:id="@+id/act_main_calorie_pb"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:layout_alignParentBottom="true"
	    >
	    <RelativeLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        >
	        <Button 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:background="@drawable/main_meal_btn_state"
	            android:id="@+id/main_meal_btn"
	            />
	        <Button 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:background="@drawable/main_box_btn_state"
	            android:id="@+id/main_box_btn"
	            android:layout_marginTop="40dp"
	            android:layout_marginLeft="65dp"
	            />
	        <Button 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:background="@drawable/main_pk_btn_state"
	            android:id="@+id/main_pk_btn"
	            android:layout_marginTop="60dp"
	            android:layout_marginLeft="125dp"
	            />
	        <Button 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:background="@drawable/main_sport_btn_state"
	            android:id="@+id/main_sport_btn"
	            android:layout_marginTop="40dp"
	            android:layout_marginLeft="225dp"
	            />
	        <Button 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:background="@drawable/main_store_btn_state"
	            android:id="@+id/main_store_btn"
	            android:layout_alignParentRight="true"
	            />
	    </RelativeLayout>
	<FrameLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    >
	    <TextView 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:id="@+id/act_main_friend_msg_tv"
	        />
	    <Button 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="right"
	        android:background="@drawable/main_community_btn_state"
	        />
	</FrameLayout>
	</LinearLayout>
</RelativeLayout>


