package com.sythealth.fitness.db.enumetype;

public enum SportType {

	THIGH("大腿运动"),
	BELLY("腹部运动"),
	CALF("小腿运动"),
	BUST("胸部运动"),
	WAIST("腰部运动"),
	ARM("手臂运动"),
	HIP("臀部运动");

	private final String displayName;

	SportType(String displayName) {
		this.displayName = displayName;
	}

	public String getDisplayName() {
		return displayName;
	}
}
