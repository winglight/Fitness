package com.sythealth.fitness.json.shopping;


/**
 * 赚取宝石列表
 */
public class FitCoinListDto {
	
	private String id;	//ID
	private String pic;	//图片URL
	private int rmb;	//人民币
	private int coin;	//对等的宝石数
	private int price;	//优惠数

	public FitCoinListDto() {
		super();
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPic() {
		return pic;
	}

	public void setPic(String pic) {
		this.pic = pic;
	}

	public int getRmb() {
		return rmb;
	}

	public void setRmb(int rmb) {
		this.rmb = rmb;
	}

	public int getCoin() {
		return coin;
	}

	public void setCoin(int coin) {
		this.coin = coin;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public FitCoinListDto(String id, String pic, int rmb, int coin, int price) {
		super();
		this.id = id;
		this.pic = pic;
		this.rmb = rmb;
		this.coin = coin;
		this.price = price;
	}

}
