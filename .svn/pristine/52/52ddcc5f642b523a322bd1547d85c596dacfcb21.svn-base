<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/home_view_bg"
    android:clickable="true"
    >
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/home_titlebar_bg"
        >
        <Button 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/back_btn_state"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="1dp"
            android:id="@+id/fra_userinfo_back_btn"
            />
        <TextView 
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="@string/fra_ui_title"
        android:textColor="@android:color/white"
        android:background="@drawable/main_titlebar_right_bg"
        android:layout_centerHorizontal="true"
        android:id="@+id/fra_userinfo_title_tv"
        />
        <Button 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/user_info_weight_rule_btn_state"
            android:layout_marginTop="4dp"
            android:id="@+id/fra_userinfo_back_btn"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            />
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/home_titlebar_highlight"
            />
    </RelativeLayout>
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        >
        <LinearLayout 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerVertical="true"
           
            >
            <LinearLayout 
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:orientation="horizontal"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="昵称"
                    android:textSize="15dp"
                    android:layout_gravity="center_vertical"
                    android:textColor="@android:color/white"
                    />
                <EditText 
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:background="@drawable/edittext_bg"
                    android:layout_gravity="center_vertical"
                    android:singleLine="true"
                    android:layout_marginRight="5dp"
                    android:textSize="15dp"
                    android:id="@+id/fra_userinfo_nickname_et"
                    />
            </LinearLayout>
            <LinearLayout 
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:orientation="horizontal"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="城市"
                    android:textSize="15dp"
                    android:layout_gravity="center_vertical"
                    android:textColor="@android:color/white"
                    />
                <TextView 
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:textColor="@android:color/black"
                    android:background="@drawable/edittext_bg"
                    android:layout_gravity="center_vertical"
                    android:singleLine="true"
                    android:layout_marginRight="5dp"
                    android:id="@+id/fra_userinfo_city_tv"
                    />
            </LinearLayout>
        </LinearLayout>
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/home_user_icon_bg"
            android:layout_centerInParent="true"
            />
        <Button 
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:background="@drawable/s_logo_shrink"
            android:layout_centerInParent="true"
            />
         <LinearLayout 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            >
            <LinearLayout 
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="性别"
                    android:textColor="@android:color/white"
                    android:textSize="15dp"
                    android:layout_gravity="center_vertical"
                    />
                <Button 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/user_info_man_btn_inactive"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:id="@+id/fra_userinfo_man_btn"
                    />
                <Button 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/user_info_woman_btn_active"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:id="@+id/fra_userinfo_woman_btn"
                    />
            </LinearLayout>
            <LinearLayout 
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="生日"
                    android:textSize="15dp"
                    android:layout_gravity="center_vertical"
                    android:textColor="@android:color/white"
                    />
                <TextView 
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:background="@drawable/edittext_bg"
                    android:layout_gravity="center_vertical"
                    android:singleLine="true"
                    android:layout_marginRight="5dp"
                    android:textSize="15dp"
                    android:id="@+id/fra_userinfo_birthday_tv"
                    />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        >
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/home_circle_bg"
            android:layout_centerHorizontal="true"
            android:layout_margin="50dp"
            />
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/fragment_userinfo_layout"
            android:orientation="vertical"
            >
            <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                >
                <LinearLayout 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/user_info_height_option_bg"
                    android:orientation="vertical"
                    >
                   <TextView 
                       android:layout_width="85dp"
                       android:layout_height="wrap_content"
                       android:hint="单位公分"
                       android:layout_gravity="bottom|center_horizontal"
                       android:layout_marginTop="35dp"
                       android:textSize="15dp"
                       android:numeric="decimal"
                       android:id="@+id/fra_userinfo_height_tv"
                       android:background="@drawable/edittext_bg"
                       /> 
                </LinearLayout>
                <LinearLayout 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="40dp"
                    android:background="@drawable/user_info_weight_option_bg"
                    >
                    <TextView 
                       android:layout_width="85dp"
                       android:layout_height="wrap_content"
                       android:hint="单位公斤"
                       android:layout_gravity="bottom|center_horizontal"
                       android:layout_marginTop="35dp"
                       android:textSize="15dp"
                       android:numeric="decimal"
                       android:id="@+id/fra_userinfo_weight_tv"
                       android:background="@drawable/edittext_bg"
                       />
                </LinearLayout>
            </LinearLayout>
            
            <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                >
                <LinearLayout 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/user_info_bmi_option_bg"
                    >
                    <TextView 
                       android:layout_width="85dp"
                       android:layout_height="wrap_content"
                       android:hint="单位XX"
                       android:layout_gravity="bottom|center_horizontal"
                       android:layout_marginTop="35dp"
                       android:textSize="15dp"
                       android:numeric="decimal"
                       android:id="@+id/fra_userinfo_bmi_tv"
                       android:background="@drawable/edittext_bg"
                       />
                </LinearLayout>
                <LinearLayout 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/user_info_bodyshape_option_bg"
                    android:layout_marginLeft="150dp"
                    >
                    <TextView 
                       android:layout_width="85dp"
                       android:layout_height="wrap_content"
                       android:layout_gravity="bottom|center_horizontal"
                       android:layout_marginTop="35dp"
                       android:textSize="15dp"
                       android:numeric="decimal"
                       android:id="@+id/fra_userinfo_bodytype_tv"
                       android:background="@drawable/edittext_bg"
                       />
                </LinearLayout>
            </LinearLayout>
            
            <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                >
                <LinearLayout 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/user_standard_weight_option_bg"
                    >
                    <TextView 
                       android:layout_width="85dp"
                       android:layout_height="wrap_content"
                       android:layout_gravity="bottom|center_horizontal"
                       android:layout_marginTop="45dp"
                       android:textSize="15dp"
                       android:numeric="decimal"
                       android:id="@+id/fra_userinfo_ruleweight_tv"
                       android:background="@drawable/edittext_bg"
                       />
                </LinearLayout>
                <LinearLayout 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/user_info_beauty_weight_option_bg"
                    android:layout_marginLeft="40dp"
                    >
                    <TextView 
                       android:layout_width="85dp"
                       android:layout_height="wrap_content"
                       android:layout_gravity="bottom|center_horizontal"
                       android:layout_marginTop="45dp"
                       android:textSize="15dp"
                       android:numeric="decimal"
                       android:id="@+id/fra_userinfo_bestweight_tv"
                       android:background="@drawable/edittext_bg"
                       />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>