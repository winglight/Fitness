package com.sythealth.fitness.json.community;

public class FitFriendCommentDto {

	private String forumid;			//帖子ID
	private String userid;			//评论者id
	private String nickname;		//评论者昵称
	private String commentuserid;	//被评论人id
	private String commentnickname;	//被评论人昵称
	private String pic;				//评论者头像
	private String createtime;		//评论时间
	private String context;			//评论内容
	private int flag;				//标识，0:评论，1：加赞	
	

	public FitFriendCommentDto() {
		super();
	}



	public FitFriendCommentDto(String forumid, String userid, String nickname,
			String commentuserid, String commentnickname, String pic,
			String createtime, String context, int flag) {
		super();
		this.forumid = forumid ;
		this.userid = userid;
		this.nickname = nickname;
		this.commentuserid = commentuserid;
		this.commentnickname = commentnickname;
		this.pic = pic;
		this.createtime = createtime;
		this.context = context;
		this.flag = flag;
	}

 

	public String getForumid() {
		return forumid;
	}



	public void setForumid(String forumid) {
		this.forumid = forumid;
	}



	public String getUserid() {
		return userid;
	}



	public void setUserid(String userid) {
		this.userid = userid;
	}



	public String getNickname() {
		return nickname;
	}



	public void setNickname(String nickname) {
		this.nickname = nickname;
	}



	public String getCommentuserid() {
		return commentuserid;
	}



	public void setCommentuserid(String commentuserid) {
		this.commentuserid = commentuserid;
	}



	public String getCommentnickname() {
		return commentnickname;
	}



	public void setCommentnickname(String commentnickname) {
		this.commentnickname = commentnickname;
	}



	public String getPic() {
		return pic;
	}



	public void setPic(String pic) {
		this.pic = pic;
	}



	public String getCreatetime() {
		return createtime;
	}



	public void setCreatetime(String createtime) {
		this.createtime = createtime;
	}



	public String getContext() {
		return context;
	}



	public void setContext(String context) {
		this.context = context;
	}

	
	public int getFlag() {
		return flag;
	}



	public void setFlag(int flag) {
		this.flag = flag;
	}
	
}
