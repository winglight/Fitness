package com.sythealth.fitness.json.user;

/**
 * 用户是否好友
 * 
 * @author tom
 *
 */
public class FitUserFriendDto {
	private String isfriend;

	public FitUserFriendDto() {
		super();
	}

	public FitUserFriendDto(String isfriend) {
		super();
		this.isfriend = isfriend;
	}

	public String getIsfriend() {
		return isfriend;
	}

	public void setIsfriend(String isfriend) {
		this.isfriend = isfriend;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result
				+ ((isfriend == null) ? 0 : isfriend.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		FitUserFriendDto other = (FitUserFriendDto) obj;
		if (isfriend == null) {
			if (other.isfriend != null)
				return false;
		} else if (!isfriend.equals(other.isfriend))
			return false;
		return true;
	}

	@Override
	public String toString() {
		return "FitUserFriendDto [isfriend=" + isfriend + "]";
	}
	
	
}
